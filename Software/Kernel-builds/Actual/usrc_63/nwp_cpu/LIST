                 *                     * A,B are lost A=rwflag, F=register, X=offset                     * F=corrected register bits [7...3]                     * A= R/W flag 1=write/ 0= read                     * X,Y untouched                     *               EBD7  spistr2   equ    *  EBD7 CE   8080               ldu    #SPIBASE  SPI base TODO  EBDA                         trfr   F,B  EBDA 1F                      fcb    $1f  EBDB F9                      fcb    F<<4|B                               endm  EBDC 5D                      tstb             register pointer  EBDD 27   09                 beq    01f       OK, valid  EBDF 34   04                 pshs   b  EBE1 C4   03                 andb   #RSVREG   reserved loctation  EBE3 5D                      tstb             xxx10xxx  is invalid!  EBE4 35   04                 puls   b  EBE6 27   1A                 beq    09f       invalid access                     * adjust READ/WRITE bit  EBE8 4D            01        tsta             cmd flag  EBE9 27   04                 beq    04f  EBEB 1A   01                 sec              write   100  EBED 20   02                 bra    05f  EBEF 1C   FE       04        clc              read    000FIO Simulation Structure             21:38:44  Feb 15, 2024   Page   75wzspi, basic spi routines                                                *  EBF1 59            05        rolb             set READ/WRITE bit  [2...0]  EBF2 58                      lslb             set variable length data [00]  EBF3 58                      lslb                     *                     * here the actual SPI action starts                     *  EBF4                         seti   disable   interrupts  EBF4 1A   50                 orcc   #FF|IF                               endm  EBF6 12                      nop                     *       lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT+SPI_IEN low  EBF7 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT low  EBF9 A7   C4                 sta    spicmd,u  EBFB AF   44                 stx    hibyta,u  and shift out  EBFD E7   44                 stb    hibyta,u  and shift out  EBFF 1A   04                 sez  EC01 39                      rts  EC02 1C   FB       09        clz  EC04 39                      rts                                          *                     * X = offset, unchanged                     * F = register#                     * read BYTE from SPI                     * return B=value, D,X,Y,U preserved                     *               EC05  byterd    equ    *  EC05 34   47                 pshs   cc,d,u  EC07 4F                      clra  EC08 8D   CD                 bsr    spistr2 >EC0A 1026 007E               lbne   09f  EC0E E6   44                 ldb    hibyta,u  extra clocks for first data                     *  EC10 E6   45                 ldb    lobyta,u  read byte  EC12 4F                      clra  EC13 ED   61                 std    1,s  EC15 20   69                 bra    sprend                                          *                     * X = offset                     * F = register#                     * read WORD from SPI                     * return D=value,  D,X,Y,U preserved                     *               EC17  wordrd    equ    *  EC17 34   47                 pshs   cc,d,u  EC19 4F                      clra >EC1A BD   EBD7               jsr    spistr2  EC1D 26   6D                 bne    09f  EC1F E6   44                 ldb    hibyta,u  extra clocks for first data                     *FIO Simulation Structure             21:38:44  Feb 15, 2024   Page   76wzspi, basic spi routines                             EC21 A6   45                 lda    lobyta,u  read byte  EC23 E6   45                 ldb    lobyta,u  read byte  EC25 ED   61                 std    1,s  EC27 20   57                 bra    sprend                                          *                     * Y=mem pointer, D=count (bytes)                     * F = register#                     * read bytes from SPI into memory location                     * return D,X,Y,U preserved                     *               EC29  bmread    equ    *  EC29 34   77                 pshs   cc,d,x,y,u  EC2B 4F                      clra  EC2C BD   F0CC               jsr    GSRRXRP  EC2F 1F   01                 tfr    d,x  EC31 CE   8080               ldu    #SPIBASE  EC34 34   40                 pshs   u  EC36 1A   50                 orcc   #$50  EC38 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT  EC3A A7   C4                 sta    spicmd,u  EC3C AF   44                 stx    hibyta,u  start address  EC3E BD   F176               jsr    tsk2rx  EC41 E7   44                 stb    hibyta,u  EC43 EC   63                 ldd    3,s       old D  EC45                         trfr   D,W       count  EC45 1F                      fcb    $1f  EC46 06                      fcb    D<<4|W                               endm  EC47 1F   12                 tfr    x,y       mem pointer  EC49 10AE 67                 ldy    7,s       old Y  EC4C E7   44                 stb    hibyta,u  shift out  EC4E 33   45                 leau   lobyta,u  EC50                         tfm4   U,Y  EC50 113B                    fdb    $113b  EC52 32                      fcb    U<<4|Y                               endm  EC53 35   40                 puls   u         old U  EC55 86   25                 lda    #SPI_RST+SPI_SR_+SPI_CR_  EC57 A7   C4                 sta    spicmd,u                     * X = old read pointer, 0,s byte wount  EC59 1F   10                 tfr    x,d  EC5B E3   61                 addd   1,s       move pointer  EC5D BD   F0DB               jsr    PSRRXRP   update pointer  EC60 35   F7                 puls   cc,d,x,y,u,pc                                          *                     * X = offset                     * F = register#