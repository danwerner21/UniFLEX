                 pshs   u  EBD3 1A   50                 orcc   #$50  EBD5 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT  EBD7 A7   C4                 sta    spicmd,u  EBD9 AF   44                 stx    hibyta,u  start address  EBDB BD   F113               jsr    tsk2rx  EBDE E7   44                 stb    hibyta,u  EBE0 EC   63                 ldd    3,s       old D  EBE2                         trfr   D,W       count  EBE2 1F                      fcb    $1f  EBE3 06                      fcb    D<<4|W                               endm  EBE4 1F   12                 tfr    x,y       mem pointer  EBE6 10AE 67                 ldy    7,s       old Y  EBE9 E7   44                 stb    hibyta,u  shift out  EBEB 33   45                 leau   lobyta,u  EBED                         tfm4   U,Y  EBED 113B                    fdb    $113b  EBEF 32                      fcb    U<<4|Y                               endm  EBF0 35   40                 puls   u         old U  EBF2 86   25                 lda    #SPI_RST+SPI_SR_+SPI_CR_  EBF4 A7   C4                 sta    spicmd,u                     * X = old read pointer, 0,s byte wount  EBF6 1F   10                 tfr    x,d  EBF8 E3   61                 addd   1,s       move pointer  EBFA BD   F078               jsr    PSRRXRP   update pointer  EBFD 35   F7                 puls   cc,d,x,y,u,pc                     FIO Simulation Structure             20:35:48  Feb 13, 2024   Page   75wzspi, basic spi routines                                                *                     * X = offset                     * F = register#                     * write BYTE to SPI, D,X,Y,U preserved                     *               EBFF  bytewr    equ    *  EBFF 34   47                 pshs   cc,d,u  EC01 86   01                 lda    #1  EC03 BD   EB74               jsr    spistr2  EC06 26   21                 bne    09f                     *  EC08 A6   62                 ld