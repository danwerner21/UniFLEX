B                               endm  F115 5A                      decb             wzenum= 1...8, sn= 0...7                     *  F116 1A   01                 sec              make reg bits + '11'  F118 59                      rolb             sock RX buffer  F119 1A   01                 sec              make reg bits + '11'  F11B 59                      rolb                     *  F11C 1C   FE                 clc  F11E 59                      rolb             read bit     '0'                     *  F11F 58                      lslb             variable size '00'  F120 58                      lslb             xxx 11 000  F121 39                      rts                                          *                     * rdsk2fb, read socket data in fifo buffer                     * Y=sock, U=sock info                     * E=socket#                     * entry D= available count                     * return, D=xferred count                     *  F122 34   76       RDSK2FB   pshs   d,x,y,u                     *  F124 10A3 4A                 cmpd   wzrqln,u  has fio max size incorporated  F127 23   02                 bls    01f  F129 EC   4A                 ldd    wzrqln,u  mandatory size  F12B 1F   01       01        tfr    D,X       sizeFIO Simulation Structure             20:35:55  Feb 13, 2024   Page   98wzspi, basic spi routines                             F12D ED   E4                 std    0,s       save xfrerred old  D  F12F ED   48                 std    wzxfer,u  F131 27   3C                 beq    15f       no data                     *  F133 BD   F069               jsr    GSRRXRP  F136 34   06                 pshs   d         save socket read pointer  F138 1F   02                 tfr    D,Y                     *  F13A CE   8080               ldu    #SPIBASE  F13D 34   01                 pshs   cc  F13F                         pshsw  F13F 1038                    fdb    $1038                               endm  F141 34   40                 pshs   u  F143 1A   50                 orcc   #$50      disable interrupts  F145 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT set CS low  F147 A7   C4                 sta    spicmd,u  F149 10AF 44                 sty    hibyta,u  start address                      >F14C BD   F113               jsr    tsk2rx    SOCK# to buffer address                       F14F E7   44                 stb    hibyta,u  F151 108E 0020               ldy    #fifo  F155 E7   44                 stb    hibyta,u  shift out first data byte  F157 33   45                 leau   lobyta,u  lobyte is the first byte shifted in  F159                         trfr   X,W  F159 1F                      fcb    $1f  F15A 16                      fcb    X<<4|W                               endm  F15B                         tfm4   U,Y  F15B 113B                    fdb    $113b  F15D 32                      fcb    U<<4|Y                               endm  F15E 35   40                 puls   u  F160 86   65                 lda    #SPI_CS_+SPI_RST+SPI_SR_+SPI_CR_  F162 A7   C4                 sta    spicmd,u  F164                         pulsw  restore   E:F  F164 1039                    fdb    $1039                               endm  F166 35   01                 puls   cc                     *  F168 35   06                 puls   d         old read pointer  F16A E3   E4                 addd   0,s       adjust transferred  F16C BD   F078               jsr    PSRRXRP   update pointer                     *  F16F 35   F6       15        puls   d,x,y,u,pc                                                               *                     * wrfb2sk,write fifo buffer to socket buffer                     * Y=sock                     * E=socket#FIO Simulation Structure             20:35:56  Feb 13, 2024   Page   99wzspi, basic spi routines                                                * on entry D= total amount to xfer                     * on return D= xferred count                     *  F171 34   76       WRFB2SK   pshs   d,x,y,u  F173 10A3 4A                 cmpd   wzrqln,u  has fio max size incorporated  F176 23   02                 bls    01f  F178 EC   4A                 ldd    wzrqln,u  F17A 1F   01       01        tfr    d,x  F17C ED   E4                 std    0,s  F17E ED   48                 std    wzxfer,u  report size  F180 27   53                 beq    15f                     *  F182 10AE C8 22              ldy    wzuwrp,u  0 if not UDP or never written  F186 26   05                 bne    24f  F188 BD   F034               jsr    GSRTXWP   get write pointer  F18B 20   02                 bra    25f  F18D 1F   20       24        tfr    y,d                     *  F18F 34   06       25        pshs   d  F191 1F   02                 tfr    D,Y                     *  F193 CE   8080               ldu    #SPIBASE  F196 34   01                 pshs   cc  F198                         pshsw  save      E:F  F198 1038                    fdb    $1038                               endm  F19A 34   40                 pshs   u  F19C 1A   50                 orcc   #$50  F19E 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT set CS low  F1A0 A7   C4                 sta    spicmd,u  F1A2 10AF 44                 sty    hibyta,u  start address                       F1A5 BD   F104               jsr    tsk2tx                       F1A8 E7   44                 stb    hibyta,u  F1AA 108E 0020               ldy    #fifo  F1AE 33   44                 leau   hibyta,u  the first to shift out  F1B0                         trfr   X,W  F1B0 1F                      fcb    $1f  F1B1 16                      fcb    X<<4|W                               endm  F1B2                         tfm3   Y,U  F1B2 113A                    fdb    $113a  F1B4 23                      fcb    Y<<4|U                               endm  F1B5 35   40                